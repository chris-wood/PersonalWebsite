---
title: Publication
---

<%
    # the `key` variable is passed in as a local variable through the page proxy (see config.rb)
    if !key
        key = "tmp"
    end
    title = data.publications.pubs[key].title
    authors = data.publications.pubs[key].authors
    venue = data.publications.pubs[key].venue
    abstract = data.publications.pubs[key].abstract
    bibtex = data.publications.pubs[key].bibtex
    paper_link = data.publications.pubs[key].links["pdf"]
    slides_link = data.publications.pubs[key].links["slides"]
    code_link = data.publications.pubs[key].links["code"]

    published = paper_link or slides_link or code_link
%>

<div class="card">
  <div class="card-content">
      <h3><%= title %></h3>
      <p><%= authors %></p>
      <p><%= venue %></p>
  </div>
</div>

<% if published %>
<h3>Materials</h3>
<ul>
    <li><p><a href="<%= paper_link %>">Paper</a></p></li>
    <li><p><a href="<%= slides_link %>">Slides</a></p></li>
    <li><p><a href="<%= code_link %>">Code</a></p></li>
</li>
</ul>

<% if abstract != nil and abstract.length > 0 %>
<article class="message">
<div class="message-header">
    <p>Abstract</p>
</div>
<div class="message-body">
<%= abstract %>
</div>
</article>
<% end %>

<% if bibtex != nil and bibtex.length > 0 %>
<article class="message">
<div class="message-header">
    <p>BibTeX</p>
</div>
<div class="blockcontent"><pre>
<%= bibtex %>
</pre></div>
</article>
<% end %>
<% end %>
